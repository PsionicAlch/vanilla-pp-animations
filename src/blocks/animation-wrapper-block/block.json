{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "vanilla-pp-animations/animation-wrapper-block",
	"version": "0.1.0",
	"title": "Animation Wrapper Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"id": {
			"type": "string"
		},
		"animation": {
			"enum": ["to", "from", "fromto", "set"],
			"default": "to"
		},
		"target": {
			"enum": ["self", "children"],
			"default": "self"
		},
		"trigger": {
			"enum": ["onscreen"],
			"default": "onscreen"
		},
		"propX": {
			"type": "string"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "animation-wrapper-block",
	"editorScript": "file:./index.js"
}
