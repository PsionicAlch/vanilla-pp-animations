{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "vanilla-pp-animations/animation-wrapper-block",
	"version": "0.1.0",
	"title": "Animation Wrapper Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"animation": {
			"enum": ["to", "from", "fromto", "set"],
			"default": "to"
		},
		"target": {
			"enum": ["self", "children"],
			"default": "self"
		},
		"trigger": {
			"enum": ["onscreen"],
			"default": "onscreen"
		},
		"propX": {
			"type": "integer"
		},
		"propY": {
			"type": "integer"
		},
		"propXPercentage": {
			"type": "boolean"
		},
		"propYPercentage": {
			"type": "boolean"
		},
		"propScaleIndividually": {
			"type": "boolean"
		},
		"propScale": {
			"type": "integer"
		},
		"propScaleX": {
			"type": "integer"
		},
		"propScaleY": {
			"type": "integer"
		},
		"propRotationType": {
			"enum": ["degrees", "radians"]
		},
		"propRotation": {
			"type": "number"
		},
		"propSkewIndividually": {
			"type": "boolean"
		},
		"propSkewType": {
			"enum": ["degrees", "radians"]
		},
		"propSkew": {
			"type": "number"
		},
		"propSkewX": {
			"type": "number"
		},
		"propSkewY": {
			"type": "number"
		},
		"propTransformOrigin": {
			"type": "string"
		},
		"propOpacity": {
			"type": "integer"
		},
		"propAutoAlpha": {
			"type": "integer"
		},
		"propDuration": {
			"type": "integer"
		},
		"propRepeatInfinitely": {
			"type": "boolean"
		},
		"propRepeat": {
			"type": "integer"
		},
		"propDelay": {
			"type": "integer"
		},
		"propStagger": {
			"type": "number"
		},
		"propYoYo": {
			"type": "boolean"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "animation-wrapper-block",
	"editorScript": "file:./index.js"
}
